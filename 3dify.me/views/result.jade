extends layout

block styles
  link(rel='stylesheet', type='text/css', href='http://www.x3dom.org/download/x3dom.css')
block content
  .main
    .main-body
      x3d(width='100%', height='800px')
        scene
          inline(url='/uploads/kermit/coloredmodel.x3d')
      .container
        .row
          .col-md-8.col-md-offset-2
            h1.brand-heading 3Dify.me
            p.intro-text
              textarea#comment.form-control(rows='2')
              | Upload photos
block footer
  .footer.text-center
    p Copyright Â© 3Dify.me
