  script.
    var jobname = !{jobname}
    var jobid = !{jobid}
  // Socket.io
  script(src='http://www.x3dom.org/download/x3dom.js', type='text/javascript')
  script(src='/socket.io/socket.io.js', type='text/javascript')
  script(src='js/result.js')